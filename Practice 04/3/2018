public class Sum_of_Arrays {
public static void main (String []args) {
	int []a= {1,2,3};
	int []b= {4,5,6};
	int[]c=new int[a.length];
	for(int i =0; i<=a.length-1;i++) {
		c[i]=a[i]+b[i];
	}
System.out.println("Sum of the arrays");
	
	for(int h =0; h<=c.length-1;h++) {
		System.out.println(c[h]);
	}
}
}
